{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "rootDirs": [
      "./src",
      "../.redwood/types/mirror/web/src"
    ],
    "paths": {
      "src/*": [
        "./src/*",
        "../.redwood/types/mirror/web/src/*"
      ],
      "types/*": [
        "./types/*",
        "../types/*"
      ]
    },
    "typeRoots": [
      "../node_modules/@types",
      "./node_modules/@types",
    ],
    "types": ["jest"],
    "jsx": "preserve",
  },
  "include": [
    "src",
    "./types/niceramps.d.ts",
    "./types/redwoodjs.d.ts",
    "../.redwood/types/includes/all-*",
    "../.redwood/types/includes/web-*",
  ]
}
