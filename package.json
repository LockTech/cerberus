{
  "private": true,
  "scripts": {
    "docker:api": "docker build -t cerberus_api:latest -t ghcr.io/locktech/cerberus_api:v0.1.0 -f ./Dockerfile.api .",
    "docker:web": "docker build -t cerberus_web:latest -f ./Dockerfile.web .",
    "docker": "cross-env yarn docker:build:api && yarn docker:build:web"
  },
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "^0.37.0-rc.60"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  }
}