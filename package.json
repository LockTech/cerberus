{
  "private": true,
  "scripts": {
    "build:dev:api": "docker build -t cerberus_api:latest -f ./Dockerfile.api .",
    "build:dev:web": "docker build -t cerberus_web:latest -f ./Dockerfile.web .",
    "build:dev": "cross-env yarn build:dev:api && yarn build:dev:web"
  },
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "^0.36.4",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  }
}
